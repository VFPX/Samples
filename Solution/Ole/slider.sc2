	PROCEDURE oleH.Scroll
		*** OLE Control Event ***
		#DEFINE TXT_LOC "slider value: "
		THISFORM.OleStatBar.Panels(2).Text = TXT_LOC + ALLTRIM(STR(THIS.value))
		THIS.Parent.spn1.Value = THIS.Value
		THIS.Parent.oleV.Value = THIS.Value
		
	ENDPROC

